{
  "experiment": {
    "name": "Rabi Oscillation",
    "description": "Single MW pulse with varying duration to observe Rabi oscillations",
    "total_time_ns": 3000,
    "sequence": [
      {
        "type": "mw_pulse",
        "start_ns": 0,
        "duration_ns": 1000,
        "omega_rabi_Hz": 5.0e6,
        "phase_rad": 0,
        "delta_Hz": 0,
        "shape": "cos"
      },
      {
        "type": "laser_readout",
        "start_ns": 1000,
        "duration_ns": 1500,
        "power": 1.0
      },
      {
        "type": "photon_counter",
        "start_ns": 1000,
        "duration_ns": 1500,
        "bin_width_ns": 5,
        "shots": 3000
      }
    ],
    "outputs": [
      {
        "type": "population_plot",
        "filename": "rabi_populations.png"
      },
      {
        "type": "photon_trace",
        "filename": "rabi_photons.png"
      }
    ]
  },
  "output_dir": "results"
}